<!DOCTYPE html>
<html>
  <%- include("./head.ejs"); %>
  <body>
    <main>
        <%- include("./header.ejs"); %>
        <h2> Create Folder</h2>
        <form method="post" ">
          <fieldset>
            <label for="folder_name">Folder Name:</label> 
            <input type="text" id="folder_name" name="folder_name" />
          </fieldset>
          <fieldset>
            <input type="submit" value="Upload" />
        </fieldset>
        </form>
        <ul>
        <% errors.forEach(function(error) { %>
          <li style="color: red;"><%= error %></li>
        <% }) %>
        </ul>
    </main>
  </body>
</html>
